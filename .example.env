# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Database Configuration
DATABASE_URL=your_database_url
DIRECT_URL=your_direct_url

# Voice Services (Required)
# DeepSeek API for speech recognition - required for voice features
DEEPSEEK_API_KEY=your_deepseek_key
# DeepSeek base URL - the app will use specific endpoints for speech recognition:
# - https://api.deepseek.com/v1/audio/transcriptions (REST API)
# - wss://api.deepseek.com/v1/audio/transcriptions/streaming (WebSocket)
DEEPSEEK_API_URL=https://api.deepseek.com

# ElevenLabs API for high-quality speech synthesis - required for voice features
ELEVEN_LABS_API_KEY=your_eleven_labs_key

# App Configuration
APP_ENV=development
DEBUG_MODE=true